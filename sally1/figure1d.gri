open default.nc netcdf
set missing value 1.0e30
set font size 12
set x size 7
set y size 6
set x margin 6
set y margin 20.5
set x axis unknown
set y axis unknown
set y name "latitude (deg)"
set x name ""
set x axis -90 -82 2
set y axis 24 31 2
set clip on
read grid x = "lon"
read grid y = "lat"
